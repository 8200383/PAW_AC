<!DOCTYPE html>
<html lang='en'>
<%- include('head'); %>
<body>
<%- include('auth') %>
<div id='dashboard' class="hidden h-screen flex overflow-hidden bg-white">
    <%- include('sidebar', { navigation }); %>

    <!-- Main column -->
    <div class="flex flex-col w-0 flex-1 overflow-hidden">
        <%- include('toggler') %>

        <main class="flex-1 relative z-0 overflow-y-auto focus:outline-none">
            <div class="px-4 py-4 sm:flex sm:items-center sm:justify-between sm:px-6 lg:px-8">
                <div class="flex-1 min-w-0">
                    <h1 id="module-label" class="text-lg font-medium leading-6 text-gray-900 sm:truncate"></h1>
                </div>
                <div class="mt-4 flex sm:mt-0 sm:ml-4">
                    <button type="button"
                            id="module-btn-action"
                            class="order-0 inline-flex items-center px-4 py-2 border border-transparent shadow-sm text-sm font-medium rounded-md text-white bg-purple-600 hover:bg-purple-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-purple-500 sm:order-1 sm:ml-3">
                    </button>
                </div>
            </div>
            <div id="container" class="align-middle inline-block min-w-full border-b border-gray-200"></div>
        </main>
    </div>

    <%- include('slideover') %>
</div>
<script src="/javascripts/module-loader.js"></script>
</body>
</html>
